# also in ifthen
\ifthenelse{test}{then clause}{else clause}
\boolean{name}
\newboolean{name}
\setboolean{name}{truth value}
\isodd{number}
\isundefined{command}
\equal{string}{string}
\AND
\OR
\NOT
\lengthtest{dimen operator dimen}

# in xifthen but not in ifthen
